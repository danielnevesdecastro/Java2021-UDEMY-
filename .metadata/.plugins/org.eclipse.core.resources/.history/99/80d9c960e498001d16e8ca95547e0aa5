package classe;

//Aula 92,Classes
public class Produto {
//Classe tipo produto

	// atributos
	String nome;
	double preco;
	double desconto;
	double precoDesconto;

	// fiz por conta própria, para ter uma função pra preçofinal
	void precoFinal(double preco, double desconto) {

		precoDesconto = preco * (1 - desconto);
		System.out.printf("Preço final com desconto: R$ %.2f", precoDesconto);

	}
}
