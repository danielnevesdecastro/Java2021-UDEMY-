package controle;

//Aula 77,78 Desafio For
public class DesafioFor {
	public static void main(String[] args) {
		// Desafio :Não pode usar valor numerico pra controlar o laço:
		/* 
		 * String valor = "#";
		 * for (int i = 1; i < 5; i++) 
		 * { System.out.println(valor); valor += "#";
		 * }
		 */
		//Minha versão
		
		
		//Versão professor 
		
		for (String v = "#"; v.equalsIgnoreCase("#####"); v += "#") {
			System.out.println(v);
		}
	}

}
