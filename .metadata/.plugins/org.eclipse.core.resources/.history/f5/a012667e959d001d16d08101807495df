package desafios.classe;

public class Pessoa {
	//Atributos
	String nome;
	double peso;
	double pesoDepois = peso;
	
	//construtor default
	Pessoa (){
		
	}
	//construtor Explitico
	Pessoa (String nome, double peso){
		this.nome = nome;
		this.peso= peso;
	}
	
	double comer(double peso) {
		return pesoDepois = this.peso + peso;
	}

}