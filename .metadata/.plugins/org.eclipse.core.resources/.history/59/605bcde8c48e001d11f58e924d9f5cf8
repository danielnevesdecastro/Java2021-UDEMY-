package controle;

import java.util.Scanner;

//Aula 80 , Switch case 
public class SwitchComBreak {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);

		String conceito = "";
		int nota;

		System.out.println("Informe a Nota");
		nota = scan.nextInt();

		switch (nota) {

		case 10:
			conceito = "A";
		case 9:
			conceito = "B";
		case 7:
			conceito = "C";

		}
		
		System.out.println("Sua nota "+nota+"É Conceito "+conceito);

	}
}
