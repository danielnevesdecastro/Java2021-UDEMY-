package fundamentos;

public class TipoString {
	
	public static void main(String[] args) {
		
		//Localizar letra do indice 0 = n do indice 
		System.out.println("olá pessoal ".charAt(0));
		
		//Concatena String
		String s = "Boa Tarde";
		System.out.println(s.concat("!!!"));
		//Concatenando usando + 
		System.out.println(s+"!!!");
	}
}
