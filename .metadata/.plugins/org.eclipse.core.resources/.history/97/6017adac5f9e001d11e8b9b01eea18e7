package arrays;

import java.util.Arrays;

// Aula 121, Arrays
public class Arrays {
	public static void main(String[] args) {
		// desclarando Array
		double[] notasAlunoA = new double[3];
		notasAlunoA[0] = 7.9;
		notasAlunoA[1] = 8;
		notasAlunoA[2] = 6.7;
		
		String arrayNota =Array.toString(notasAlunoA);
		System.out.println();

	}
}
