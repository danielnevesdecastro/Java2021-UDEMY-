package classe;

//Aula 92 - 96,Classes
public class Produto {
//Classe tipo produto

	// atributos
	String nome;
	double preco;
	double desconto;

	// método desconto
	double precoDesconto(double descontoGerente) {
		
		return preco * (1 - (desconto + descontoGerente));

	}
	
	// Criando Construtor (parâmetro explícito)
	Produto(String nomeInicial){
		nome = nomeInicial;
	}
	// Criando Construtor default (parâmetro implícito) para q se possa usar os dois contrutores 
	void Produto(){
		
	}
}
