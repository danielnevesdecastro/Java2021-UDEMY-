package classe;

//Aula 92 - 96,Classes
public class Produto {
//Classe tipo produto

	// atributos
	String nome;
	double preco;
	double desconto;

	// método desconto
	double precoDesconto(double descontoGerente) {
		
		return preco * (1 - (desconto + descontoGerente));

	}
	
	// Criando Construtor (parâmetro explícito)
	Produto(String nomeInicial, double precoInicial ,double  ){
		nome = nomeInicial;
		preco = precoInicial;
	}
	// Criando Construtor default (parâmetro implícito) para q se possa usar os dois contrutores 
	//se o contrutor tiver qualquer retorno , mesmo void , passa a ser um método
	Produto(){
		
	}
}
