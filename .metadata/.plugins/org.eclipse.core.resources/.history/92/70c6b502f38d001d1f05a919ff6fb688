package controle;

public class For3 {
	public static void main(String[] args) {
		// i contador sendo declarado fora ,
		// para q a variável fique visivel fora do laço for
		int i;

		for (i = 0; i < 10; i++) {
			System.out.println("Nº " + i);

		}
		System.out.println("Nº fora " + i);
		
		int cont = 1;
		for (int j = 0; j <=10; j++) {
			for (int k = 0 ; k<=10;k++ ) {
				System.out.printf("\nNº%d %d , %d",cont,j,k);
				cont++;
			}
		}
	}
}
